{
  "type": "module",
  "name": "controlthemall",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "echo \"LOL: RUNNING START\"",
    "serve": "run-s vue:serve",
    "backend": "run-s backend:serve",
    "build": "run-s tauri:build",
    "package": "run-s backend:package",
    "dev": "run-s tauri:serve",
    "vue": "vue-cli-service",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "vue:lint": "vue-cli-service lint",
    "tauri:serve": "run-p vue:serve tauri:cargo:serve",
    "tauri:cargo:serve": "cargo tauri dev --config '{\"build\":{\"devPath\":\"http://localhost:8080/\"}}'",
    "tauri:serve:simple": "echo \"CURRENTLY NOT SUPPORTED BUT SHOULD WORK ONCE A NEW RELEASE COMES OUT!\" && vue-cli-service tauri:serve",
    "tauri:build": "vue-cli-service tauri:build --target aarch64-apple-darwin --debug",
    "backend:serve": "nodemon ./backend -w ./backend",
    "backend:package": "run-s backend:package:ncc backend:package:caxa backend:move-binary",
    "backend:package:ncc": "ncc build backend/index.js -o _package/ncc",
    "backend:package:caxa": "caxa --input \"_package/ncc\" --output \"_package/binary/backend\" -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/index.js\"",
    "backend:package:pkg": "echo \"Error: currently not supported because ESM module support is not supported as of version pkg@5.5.1\" && exit 1; pkg _package/ncc/index.js --output _package/app",
    "backend:move-binary": "node scripts/move-binary.js",
    "clean": "shx rm -rf _package",
    "clean:tauri": "shx rm -rf src-tauri/binaries src-tauri/target",
    "cu": "ncu",
    "cu:fix": "ncu -u",
    "format": "prettier . --write --ignore-unknown",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@elgato-stream-deck/node": "^5.1.2",
    "@ionic/vue": "^6.0.1",
    "@ionic/vue-router": "^6.0.1",
    "@julusian/jpeg-turbo": "^1.1.1",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@nut-tree/nut-js": "^2.0.0-RC1",
    "@sindresorhus/is": "^4.2.0",
    "@tauri-apps/api": "^1.0.0-beta.8",
    "atem-connection": "^3.0.0-nightly-latest-20211125-222857-2b30eea.0",
    "atem-state": "^0.12.2",
    "easymidi": "^2.1.0",
    "fastq": "^1.13.0",
    "get-port": "^6.0.0",
    "http-graceful-shutdown": "^3.1.5",
    "inf-ee": "^1.0.4",
    "ionicons": "^6.0.0",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "ky": "^0.28.7",
    "ky-universal": "^0.9.1",
    "lodash-es": "^4.17.21",
    "random-int": "^3.0.0",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^6.0.0-rc.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@vercel/ncc": "^0.33.1",
    "@vue/cli": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "caxa": "^2.1.0",
    "execa": "^6.0.0",
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.0.5",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pkg": "^5.5.1",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "vue-cli-plugin-tauri": "^1.0.0-beta.6"
  },
  "description": "",
  "homepage": "https://github.com/chrisspiegl/ControlThemAll",
  "repository": "https://github.com/chrisspiegl/ControlThemAll",
  "license": "UNLICENSED",
  "author": {
    "name": "Chris Spiegl",
    "email": "chris@chrisspiegl.com",
    "url": "https://chrisspiegl.com/"
  }
}
